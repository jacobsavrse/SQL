#Use a select statement to order items by price 
select * from superstore
order by price;

#Use a select statement to view the sum of the price of the items
select sum(price) 
from superstore;

#Use a select statement for the average price from Kitchen Supplies
select avg(price)
from superstore
where category="Kitchen Supplies";

#Use a select statement to rename the columns for Electronics and Appliances
and to order the results by Category
select item_name as Item, category as Category, price as Price
from superstore
where category="Electronics" or category ="Appliances"
order by category
